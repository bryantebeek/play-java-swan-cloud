# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


GET     /                           controllers.SwanController.index

# Map static resources from the /public folder to the /assets URL path
# GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

POST    /swansong                   controllers.SwanController.swanSongFormService

POST    /swansongjson                   controllers.SwanController.swanSongJSONService

GET    /push                       controllers.SwanController.testWebService

GET    /registerrainvalue                   controllers.SwanController.testRegisterRainValueSwan

GET    /unregisterrainvalue                  controllers.SwanController.testUnregisterRainValueSwan


GET    /registerraintristate                   controllers.SwanController.testRegisterRainTriStateSwan


GET    /unregisterraintristate                  controllers.SwanController.testUnregisterRainTriStateSwan


GET    /registertestvalue                   controllers.SwanController.testRegisterTestValueSwan

GET    /unregistertestvalue                  controllers.SwanController.testUnregisterTestValueSwan


GET    /registertesttristate                   controllers.SwanController.testRegisterTestTriStateSwan


GET    /unregistertesttristate                  controllers.SwanController.testUnregisterTestTriStateSwan



GET    /registercurrencyvalue                   controllers.SwanController.testRegisterCurrencyValueSwan

GET    /unregistercurrencyvalue                  controllers.SwanController.testUnregisterCurrencyValueSwan


GET    /registercurrencytristate                   controllers.SwanController.testRegisterCurrencyTriStateSwan


GET    /unregistercurrencytristate                  controllers.SwanController.testUnregisterCurrencyTriStateSwan





GET    /registerall                             controllers.SwanController.testRegisterAll

GET    /unregisterall                         controllers.SwanController.testUnregisterAll


POST    /registeremail                             controllers.SwanController.registerExpressionForEmailNotification

POST    /unregisteremail                             controllers.SwanController.unRegisterExpressionForEmailNotification


GET /webhook/                                       controllers.SwanController.setupWebHook


POST /webhook/                              controllers.SwanController.requestWebHookSwanBot


POST /swan/register/                              controllers.SwanController.swanPhoneRegister

POST /swan/unregister/                          controllers.SwanController.swanPhoneUnregister



GET    /registerloravalue                   controllers.SwanController.testRegisterLoraValueSwan

GET    /unregisterloravalue                  controllers.SwanController.testUnregisterLoraValueSwan


GET    /sensors                  controllers.SwanController.testGetAllSensors
