# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


GET     /                           controllers.SwanController.index

# Map static resources from the /public folder to the /assets URL path
# GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

POST    /swansong                   controllers.SwanController.swanSongFormService

POST    /swansongjson                   controllers.SwanController.swanSongJSONService

GET    /push                       controllers.SwanController.testWebService

GET    /registerrainvalue                   controllers.SwanController.testRegisterRainValueSwan

GET    /unregisterrainvalue                  controllers.SwanController.testUnregisterRainValueSwan


GET    /registerraintristate                   controllers.SwanController.testRegisterRainTriStateSwan


GET    /unregisterraintristate                  controllers.SwanController.testUnregisterRainTriStateSwan


GET    /registertestvalue                   controllers.SwanController.testRegisterTestValueSwan

GET    /unregistertestvalue                  controllers.SwanController.testUnregisterTestValueSwan


GET    /registertesttristate                   controllers.SwanController.testRegisterTestTriStateSwan


GET    /unregistertesttristate                  controllers.SwanController.testUnregisterTestTriStateSwan