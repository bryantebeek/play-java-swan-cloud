# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


GET     /                           controllers.SwanController.index

# Map static resources from the /public folder to the /assets URL path
# GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

POST    /swansong                   controllers.SwanController.swanSongFormService

POST    /swansongjson                   controllers.SwanController.swanSongJSONService

GET    /push                       controllers.SwanController.testWebService

GET    /registervalue                   controllers.SwanController.testRegisterValueSwan

GET    /unregistervalue                  controllers.SwanController.testUnregisterValueSwan


GET    /registertristate                   controllers.SwanController.testRegisterTriStateSwan


GET    /unregistertristate                  controllers.SwanController.testUnregisterTriStateSwan